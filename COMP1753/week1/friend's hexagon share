
import turtle as t

t.pensize(8)
t.pencolor('orange')
t.penup()
t.goto(200,0)
t.pendown()
t.fillcolor('green')
t.begin_fill()
t.goto(400,0)
t.left(60)
t.forward(200)
t.left(60)
t.forward(200)
t.left(60)
t.forward(200)
t.left(60)
t.forward(200)
t.left(60)
t.forward(200)
t.end_fill()
t.exitonclick()





