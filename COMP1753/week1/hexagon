
import turtle as t

t.goto(0,0)
t.fillcolor('orange')
t.begin_fill()
t.goto(200,0)
t.goto(300,200)
t.goto(200,400)
t.goto(0,400)
t.goto(-100,200)
t.goto(0,0)
t.end_fill()
t.penup()
t.goto(25,25)
t.pendown()
t.fillcolor('green')
t.begin_fill()
t.goto(175,25)
t.goto(263.5,200)
t.goto(175,375)
t.goto(25,375)
t.goto(-63.5,200)
t.goto(25,25)
t.end_fill()

t.exitonclick()

